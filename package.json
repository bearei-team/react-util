{
  "name": "@bearei/react-icon",
  "version": "0.0.4",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./lib-esm/index.js",
  "author": "bearei <bear_ei@outlook.com>",
  "description": "A basic icon component that supports react and native react.",
  "license": "MIT",
  "readmeFilename": "README.md",
  "keywords": [
    "react",
    "react native",
    "typescript",
    "icon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bearei-team/react-icon"
  },
  "bugs": {
    "url": "https://github.com/bearei-team/react-icon/issues"
  },
  "dependencies": {
    "@bearei/react-util": "^0.0.15",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-native": "^0.70.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "babel-loader": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.7.1",
    "react-dom": "^18.2.0",
    "react-native": "^0.70.6",
    "shx": "^0.3.4",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "files": [
    "lib",
    "lib-esm"
  ],
  "scripts": {
    "build": "yarn clean && cross-env MODE=\"production\" yarn build:cjs && cross-env MODE=\"production\" yarn build:esm",
    "build:dev": "yarn clean && cross-env MODE=\"development\" yarn build:cjs && cross-env MODE=\"development\" yarn build:esm",
    "build:esm": "cross-env TS_NODE_PROJECT=\"tsconfig.esm.json\" LIBRARY=\"module\"  webpack --progress",
    "build:cjs": "cross-env TS_NODE_PROJECT=\"tsconfig.cjs.json\" LIBRARY=\"commonjs\" webpack --progress",
    "clean": "shx rm -rf lib-esm lib",
    "test": "jest --watchAll --detectOpenHandles",
    "coverage": "yarn test --coverage",
    "lint": "eslint . --ext .jsx,.ts,.tsx",
    "fix": "yarn lint --fix",
    "publish": "yarn build && yarn npm publish --access public"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@3.2.4"
}
